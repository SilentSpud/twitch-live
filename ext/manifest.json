  "icons": {
    "128": "images/icon-128.png",
    "16": "images/icon-16.png",
    "48": "images/icon-48.png"
  },
  "manifest_version": 2,
  "name": "Twitch Live",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
